public class Numero {

	 public static String dividir(int a, int b){
        String resultado = "";
        int r = 0;
        try{
            r = a/b;
        }catch (Exception e){
            resultado = "Não é possivel dividir por zero \n";
        }finally {
            resultado += a + " / " + b + " = " + r + "\n";
        }
         return resultado;
    }
}
